{
  "name": "workspace-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "prettier": "prettier --write ./**/*.{js,vue,css,scss}",
    "eslint": "eslint ./**/*.{js,vue} --color --fix",
    "stylelint:css": "stylelint ./src/**/*.{css,scss} --custom-syntax postcss-scss --aei --color --fix",
    "stylelint:vue": "stylelint ./src/**/*.vue --custom-syntax postcss-html --aei --color --fix",
    "lint": "npm run eslint & npm run stylelint:css & npm run stylelint:vue",
    "husky:prepare": "husky install",
    "commit": "git-cz"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "core-js": "^3.8.3",
    "vue": "^2.6.14",
    "vue-router": "^3.5.1",
    "vuetify": "^2.6.0",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@mdi/font": "^6.6.96",
    "@babel/eslint-parser": "^7.17.0",
    "@commitlint/cli": "^16.2.3",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "commitizen": "^4.2.4",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "eslint-plugin-vuejs-accessibility": "^1.1.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "postcss": "^8.4.12",
    "postcss-html": "^1.3.0",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.6.2",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "stylelint": "^14.6.1",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "vue-cli-plugin-vuetify": "^2.4.8",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "lint-staged": {
    "*.js": [
      "npm run eslint"
    ],
    "*.vue": [
      "npm run eslint",
      "npm run stylelint:vue"
    ],
    "*.{css,scss}": [
      "npm run stylelint:scss"
    ]
  }  
}
